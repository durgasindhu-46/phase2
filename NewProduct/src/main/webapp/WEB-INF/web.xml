<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>App</servlet-name>
  	<display-name>App</display-name>
  	<description></description>
  	<servlet-class>com.training.App</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>students</servlet-name>
  	<display-name>students</display-name>
  	<description></description>
  	<servlet-class>com.training.students</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Product</servlet-name>
  	<display-name>Product</display-name>
  	<description></description>
  	<servlet-class>beans.Product</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductDao</servlet-name>
  	<display-name>ProductDao</display-name>
  	<description></description>
  	<servlet-class>dao.ProductDao</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductService</servlet-name>
  	<display-name>ProductService</display-name>
  	<description></description>
  	<servlet-class>service.ProductService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertController</servlet-name>
  	<display-name>InsertController</display-name>
  	<description></description>
  	<servlet-class>com.business.InsertController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>App</servlet-name>
  	<url-pattern>/App</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>students</servlet-name>
  	<url-pattern>/students</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Product</servlet-name>
  	<url-pattern>/Product</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductDao</servlet-name>
  	<url-pattern>/ProductDao</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductService</servlet-name>
  	<url-pattern>/ProductService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertController</servlet-name>
  	<url-pattern>/InsertController</url-pattern>
  </servlet-mapping>
</web-app>
